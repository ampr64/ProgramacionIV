<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.MainActivity">

    <TextView
        android:id="@+id/main_tv_result"
        android:layout_width="348dp"
        android:layout_height="20dp"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="62dp" />

    <Button
        android:id="@+id/main_btn_signUp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="@string/main_animal_signup"
        app:layout_constraintBaseline_toBaselineOf="@+id/main_btn_examine"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/main_btn_examine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginTop="209dp"
        android:text="@string/main_new_examination"
        app:layout_constraintStart_toEndOf="@+id/main_btn_signUp"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>